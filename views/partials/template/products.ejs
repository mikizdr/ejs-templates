<div class="container">
  <div class="row">
    <% products.forEach(function(item){ %>
    <div class="col-sm-6">
      <div class="thumbnail"> <img src="/images/chesterfield/600x300/<%= item.images %>" alt="<%= item.title %>" class="img-responsive">
        <div class="caption">
          <h3><%= item.title %></h3>
          <p><%= item.short_desc %></p>
          <hr>
          <p><a href="#" class="btn btn-success" role="button">Detaljnije</a></p>
        </div>
      </div>
    </div>
    <% }) %>
</div>
